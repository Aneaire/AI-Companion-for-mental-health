{
  "name": "source",
  "module": "server/index.ts",
  "scripts": {
    "start": "bun server/index.ts",
    "dev": "bun --watch server/index.ts",
    "build": "cd frontend && bun install && bun run build",
    "render-build": "bun install && cd frontend && bun install && bun run build",
    "postinstall": "cd frontend && bun install",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit generate && drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev:frontend": "cd frontend && bun run dev",
    "dev:server": "bun --watch server/index.ts",
    "dev:all": "concurrently \"bun run dev:frontend\" \"bun run dev:server\""
  },
  "type": "module",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/bun": "latest",
    "@types/pg": "^8.15.4",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "concurrently": "^8.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@clerk/backend": "^2.12.1",
    "@clerk/clerk-sdk-node": "^5.1.6",
    "@google/generative-ai": "^0.24.1",
    "@hono/zod-validator": "^0.5.0",
    "@supabase/supabase-js": "^2.49.10",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.7.9",
    "jose": "^6.1.0",
    "pg": "^8.16.0",
    "postgres": "^3.4.7",
    "react-markdown": "^10.1.0",
    "recharts": "^3.1.2",
    "rehype-highlight": "^7.0.2",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.24.4",
    "zustand": "^5.0.5"
  }
}
